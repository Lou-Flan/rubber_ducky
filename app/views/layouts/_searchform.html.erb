<%= search_form_for @search do |f| %>
  <%= f.label :name_or_description_cont %>
  <%= f.search_field :name_or_description_cont %>
  <%#= f.label :language_cont %>
  <%#= f.search_field :experiences_language_cont %>

    <% Experience.all.each do |language| %>
        <%= check_box_tag('q[experiences_language_cont_any][]', language.language) %>
            <%= language.language %>
    <% end %>

  <%= f.submit "search" %>
  <%= link_to "Reset Search", listings_path %>
<% end %>




